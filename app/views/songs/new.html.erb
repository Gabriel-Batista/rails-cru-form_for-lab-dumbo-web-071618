<h1>New Songs Form</h1>

<%= form_for :song, url: "/songs" do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :artist_id %>
    <%= f.number_field :artist_id%><br>
    <%= f.label :genre_id %>
    <%= f.text_field :genre_id %><br>
    <%= f.submit "Create Song" %>
<% end %>